[
    {
        "cpu": 0,
        "essential": true,
        "image": "${IMAGE}",
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "/ecs/${ENVIRONMENT}-${PROJECT_NAME}-chat",
                "awslogs-create-group": "true",
                "awslogs-region": "${AWS_REGION}",
                "awslogs-stream-prefix": "ecs"
            }
        },
        "healthCheck": {
            "command": ["CMD-SHELL", "curl -f http://localhost:8002/healthz || exit 1"],
            "interval": 300,
            "timeout": 5,
            "retries": 3,
            "startPeriod": 60
        },
        "mountPoints": [],
        "name": "chat",
        "portMappings": [
            {
                "containerPort": 8002,
                "protocol": "tcp"
            }
        ],
        "systemControls": [],
        "volumesFrom": []
    }
]